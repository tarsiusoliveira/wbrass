<p id="notice"><%= notice %></p>

<h1>Sources</h1>

<table>
  <thead>
    <tr>
      <th>X</th>
      <th>Y</th>
      <th>Z</th>
      <th>Numrays</th>
      <th>Rotaz</th>
      <th>Rotel</th>
      <th>Spl1m</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sources.each do |source| %>
      <tr>
        <td><%= source.x %></td>
        <td><%= source.y %></td>
        <td><%= source.z %></td>
        <td><%= source.numRays %></td>
        <td><%= source.rotAz %></td>
        <td><%= source.rotEl %></td>
        <td><%= source.spl1m %></td>
        <td><%= source.name %></td>
        <td><%= link_to 'Show', source %></td>
        <td><%= link_to 'Edit', edit_source_path(source) %></td>
        <td><%= link_to 'Destroy', source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Source', new_source_path %>
